{
  "name": "Rust Codespaces",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers/features/rust:1": {
      "version": "latest",
      "profile": "minimal"
    }
  },
  "extensions": [
    "rust-lang.rust-analyzer",
    "serayuzgur.crates",
    "tamasfe.even-better-toml",
    "jdinhlife.gruvbox",
    "jonathanharty.gruvbox-material-icon-theme"
  ]
}
